    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-fetch-mock": "3.0.3",
    "lint-staged": "15.2.2",
    "patch-package": "6.2.2",
    "prettier": "3.2.5",
    "react-native-svg-transformer": "1.3.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.1",
    "typescript": "5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "directories": {
    "test": "tests"
  }
