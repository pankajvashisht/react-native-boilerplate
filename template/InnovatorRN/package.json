    "@react-navigation/native-stack": "6.9.22",
    "@react-navigation/stack": "6.3.25",
    "@tanstack/react-query": "5.28.9",
    "axios": "1.6.8",
    "i18n-js": "^3.8.0",
    "mixpanel-react-native": "2.4.1",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-native-bootsplash": "5.4.1",
    "react-native-code-push": "8.2.1",
    "react-native-config": "1.5.1",
    "react-native-element-dropdown": "^2.10.4",
    "react-native-error-boundary": "1.2.4",
    "react-native-gesture-handler": "2.15.0",
    "react-native-keychain": "8.1.3",
    "react-native-popup-menu": "^0.16.1",
    "react-native-reanimated": "3.9.0",
    "react-native-restart": "0.0.27",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "3.29.0",
    "react-native-size-matters": "^0.4.0",
    "react-native-svg": "14.1.0"
