{
  "name": "innovatorrn",
  "version": "0.0.1-1",
  "private": true,
  "scripts": {
    "androidStagingDebug": "npx react-native run-android --mode=stagingDebug --appIdSuffix=staging",
    "androidProductionDebug": "npx react-native run-android --mode=productionDebug --appIdSuffix=production",
    "iosStagingDebug": "npx react-native run-ios --scheme 'InnovatorRN_Staging'",
    "iosProductionDebug": "npx react-native run-ios --scheme 'InnovatorRN_Production'",
    "start": "npx react-native start",
    "test": "npx cross-env NODE_ENV=test jest -i -ci",
    "lint": "eslint .",
    "pod": "npx pod-install ios",
    "checkForbiddenWords": "node ./scripts/checkForbiddenWords.js",
    "testSnapshots": "yarn test .snap.spec.js",
    "testUnits": "yarn test .unit.spec.js",
    "postinstall": "yarn pod",
    "checkRevieweeCommitStatus": "bash ./scripts/checkRevieweeCommitStatus.sh",
    "prepare": "cd .. && husky InnovatorRN/.husky"
  },
  "dependencies": {
    "@codler/react-native-keyboard-aware-scroll-view": "^2.0.1",
    "@react-native-async-storage/async-storage": "^1.16.1",
    "@react-native-community/netinfo": "11.3.1",
